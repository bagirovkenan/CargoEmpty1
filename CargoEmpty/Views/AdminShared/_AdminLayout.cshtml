                </div>
                <div class="col-12 col-lg-12 RightSrcInput" style="margin-bottom:-40px;">
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-6 col-12">
                            <label>Sifaris:</label>
                            <input id="MainPageOrderSerchInput" class="form-control" type="search" name="name" value="" placeholder="Search" />

                            <div id="MainPageOrderSerchDiv" class="row">
                                @*bura ajax ile dolacaq*@
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 col-12" style="position:relative;">

                            <label>Isdifadeci:</label>
                            <input id="MainPageUserSerchInput" class="form-control" type="search" name="name" value="" placeholder="Search" />
                            <div id="MainPageUserSerchDiv" class="row">
                                @*bura ajax ile dolacaq*@
                            </div>
                            @*@Session["FirstName"] @Session["LastName"] | @Session["UserCode"]*@
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 col-12">
                            <div class="dropdown">
                                <button style="margin-top:32px;width:100%;" class="btn btn-light dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    @Session["FirstName"] @Session["LastName"] | @Session["UserCode"]
                                </button>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="/UserLogin/ExitAccount">Log Out</a>                                  
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 col-12">
                            <button class="btn btn-warning" id="layoutDeclerationModal" style="width:100%;box-shadow:unset;margin-top:31px;">Beyanname</button>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>

